{
  "containerDefinitions": [
    {
      "name": "myapp",
      "image": "418295688701.dkr.ecr.ap-northeast-2.amazonaws.com/plus-project-jeontak:latest",
      "cpu": 256,
      "memory": 300,
      "portMappings": [
        {
          "containerPort": 8080,
          "hostPort": 8080,
          "protocol": "tcp"
        }
      ],
      "essential": true,
      "environment": [
        {
          "name": "SPRING_DATASOURCE_USERNAME",
          "value": "root"
        },
        {
          "name": "SPRING_REDIS_PORT",
          "value": "6379"
        },
        {
          "name": "SPRING_DATASOURCE_URL",
          "value": "jdbc:mysql://db:3306/plus-project"
        },
        {
          "name": "SPRING_DATASOURCE_PASSWORD",
          "value": "1111"
        },
        {
          "name": "SPRING_REDIS_HOST",
          "value": "redis"
        }
      ],
      "mountPoints": [],
      "volumesFrom": [],
      "dependsOn": [
        {
          "containerName": "db",
          "condition": "HEALTHY"
        },
        {
          "containerName": "redis",
          "condition": "HEALTHY"
        }
      ],
      "systemControls": []
    },
    {
      "name": "db",
      "image": "mysql:8.0",
      "cpu": 256,
      "memory": 512,
      "portMappings": [
        {
          "containerPort": 3306,
          "hostPort": 3306,
          "protocol": "tcp"
        }
      ],
      "essential": true,
      "environment": [
        {
          "name": "MYSQL_DATABASE",
          "value": "plus-project"
        },
        {
          "name": "MYSQL_ROOT_PASSWORD",
          "value": "1111"
        }
      ],
      "mountPoints": [],
      "volumesFrom": [],
      "healthCheck": {
        "command": [
          "CMD-SHELL",
          "mysqladmin ping -h 127.0.0.1 -u root -p$MYSQL_ROOT_PASSWORD"
        ],
        "interval": 15,
        "timeout": 5,
        "retries": 10,
        "startPeriod": 30
      },
      "systemControls": []
    },
    {
      "name": "redis",
      "image": "redis:latest",
      "cpu": 128,
      "memory": 128,
      "portMappings": [
        {
          "containerPort": 6379,
          "hostPort": 6379,
          "protocol": "tcp"
        }
      ],
      "essential": true,
      "environment": [],
      "mountPoints": [],
      "volumesFrom": [],
      "healthCheck": {
        "command": [
          "CMD",
          "redis-cli",
          "ping"
        ],
        "interval": 15,
        "timeout": 5,
        "retries": 10,
        "startPeriod": 15
      },
      "systemControls": []
    }
  ],
  "family": "plus-project-task",
  "executionRoleArn": "arn:aws:iam::418295688701:role/ecsTaskExecutionRole",
  "networkMode": "bridge",
  "volumes": []
}